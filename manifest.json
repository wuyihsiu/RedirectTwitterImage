{
    "manifest_version": 3,
    "name": "Redirect twitter image to original size",
    "version": "1.0.0",
    "description": "Redirect twitter image to original size",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "redirect_twitter_image",
                "enabled": true,
                "path": "redirect_twitter_image.json"
            }
        ]
    },
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "*://pbs.twimg.com/*"
    ]
}